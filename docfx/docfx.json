{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "../artifacts/bin/RazorConsole.Core/release_net10.0",
          "files": [
            "**.dll"
          ]
        }
      ],
      "output": "../website/src/.docfx"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "docfx",
      "_appTitle": "docfx",
      "_enableSearch": false,
      "pdf": false
    }
  }
}